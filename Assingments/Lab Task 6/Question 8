#include<stdio.h>
int main (){
int i=65;
for (i=65;i<=69;i++){
    if (i==65 || i==69){
        printf("%c %c %c %c %c \n",i,i+1,i+2,i+3,i+4,i+5); 
    }
    else if (i==66 || i==68)
    printf ("  %c   %c \n",i+1,i+4);
    else
        printf ("    %c  \n",i+2);
        
}
}
//Alternative:
 
#include<stdio.h>
int main (){
int i=65;
for (i=65;i<=70;i++){
    if (i==65 || i==69){
        printf("* * * * *\n"); 
    }
    else if (i==66 || i==68)
    printf("  *   *  \n"); 
    else
        printf("    *    \n"); 
}
}
